#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;

    while(t--){
        int a[5],cnt=0;
        for(int i=0;i<5;i++){
            cin>>a[i];
            if(a[i]==1){
                cnt++;
            }
        }
        if(cnt>=4){
            cout<<"YES"<<endl;
        }
        else{
            cout<<"NO"<<endl;
        }
    }
    return 0;
}
